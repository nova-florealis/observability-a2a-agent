{
  "name": "observability-a2a-agent",
  "version": "1.0.0",
  "description": "A2A Observability GPT Agent with client/server architecture",
  "main": "dist/agent.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/agent.js",
    "dev": "node --loader ts-node/esm src/agent.ts",
    "client": "node --loader ts-node/esm src/client.ts",
    "debug:agent": "node --loader ts-node/esm src/agent.ts",
    "debug:client": "node --loader ts-node/esm src/client.ts"
  },
  "keywords": [
    "a2a",
    "observability",
    "gpt",
    "nevermined",
    "helicone",
    "openai"
  ],
  "dependencies": {
    "@nevermined-io/payments": "file:./payments",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "openai": "^4.47.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}